<%- include("comp/header")  %>
<div class="w-full min-h-screen bg-zinc-800 text-white">
    <div class="cards flex flex-wrap gap-10 p-5">
        <% posts.forEach(elem => { %>
            
            <div class=" w-32  border-transparent outline-none">
                <div class="card w-full h-40 bg-sky-200 rounded-xl overflow-hidden">
                    <img class="w-full h-full object-cover"  src="/images/uploads/<%= elem.image  %>" alt="">
                </div>
                <h5 class="text-sm mt-2"><%= elem.title %></h5>
            </div>

        <% });  %>
    </div>
</div>
<%- include("comp/footer")  %>